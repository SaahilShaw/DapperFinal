@model IEnumerable<DapperFinal.Models.StudentSub>
<style>
    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    td, th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
</style>
<h1>Multi Table</h1>

<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th>
           Subject Name
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Subject_name
            </td>

        </tr>
    }

</table>
@Html.ActionLink("GO BACK", "BackButtonForMenu", "Route")
